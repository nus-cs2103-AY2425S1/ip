<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="600.0" style="-fx-background-color: #FFFF69; -fx-border-color: #000000; -fx-vgap: 5px;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="CENTER" minHeight="-Infinity" text="Label" wrapText="true">
            <font>
                <Font name="Comic Sans MS" size="16.0" />
            </font></Label>
