<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_RIGHT"
         maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="48.0"
         prefWidth="400.0" type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/22">
    <children>
        <Label fx:id="dialog" minHeight="-Infinity"
               stylesheets="@../css/main.css" text="Label" wrapText="true">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </Label>
        <Separator opacity="0.0" orientation="VERTICAL" prefHeight="5.0"
                   prefWidth="8.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <padding>
                <Insets left="2.0" right="2.0"/>
            </padding>
        </Separator>
        <Circle fx:id="profilePicture" fill="DODGERBLUE" radius="17.0"
                stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0"/>
    </children>
    <padding>
        <Insets bottom="3.0" left="10.0" right="10.0" top="3.0"/>
    </padding>
</fx:root>
