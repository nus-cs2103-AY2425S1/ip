<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.layout.StackPane?>

<fx:root alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" spacing="15"
         type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
    <Label fx:id="dialog" minHeight="-Infinity" text="Label" wrapText="true"/>
    <StackPane>
        <Circle radius="27" fill="transparent" stroke="darkgray" strokeWidth="2"/>
        <ImageView fx:id="displayPicture" fitWidth="50.0" fitHeight="50.0">
            <clip>
                <Circle radius="25.0" centerX="25.0" centerY="25.0"/>
            </clip>
        </ImageView>
    </StackPane>
</fx:root>
