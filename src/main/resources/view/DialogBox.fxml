<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>

<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" prefWidth="400.0" spacing="20.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox fx:id="messageBox" alignment="CENTER_LEFT" styleClass="message-box" stylesheets="@../style/DialogBox.css">
            <children>
                <Label fx:id="dialog" maxWidth="390" styleClass="message-box-label" stylesheets="@../style/DialogBox.css" wrapText="true">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </padding>
                </Label>
            </children>
        </HBox>
        <Circle fx:id="displayPicture" fill="DODGERBLUE" radius="30.0" stroke="BLACK" strokeType="INSIDE" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
